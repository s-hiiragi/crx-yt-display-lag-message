{
    "manifest_version" : 3,
    "name" : "YouTube Display Lag Message",
    "description" : "Display a message when a live stream is lagged",
    "version" : "1.5",
    "icons": {
        "48": "images/icon-48.png"
    },

    "host_permissions": [
        "https://www.youtube.com/*"
    ],

    "content_scripts" : [
        {
            "js" : ["content_script.js"],
            "matches" : ["https://www.youtube.com/*"]
        }
    ]
}
