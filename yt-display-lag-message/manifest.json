{
    "manifest_version" : 3,
    "name" : "YouTube Display Lag Message",
    "description" : "Display a message when a live stream is lagged",
    "version" : "1.0",

    "host_permissions": [
        "https://www.youtube.com/*"
    ],

    "content_scripts" : [
        {
            "js" : ["content_script.js"],
            "matches" : ["https://www.youtube.com/watch*"]
        }
    ]
}
